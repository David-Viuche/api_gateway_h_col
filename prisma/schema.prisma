// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Patient {
  patientId Int    @id @default(autoincrement())
  id        String @unique
  firstName String
  lastName  String
  email     String @unique
  phone     String
  address   String
  city      String
}
